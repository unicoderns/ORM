{
  "name": "@unicoderns/orm",
  "version": "0.4.6",
  "description": "Unicoderns Object/Relational Mapping",
  "author": {
    "name": "Unicoderns S.A.",
    "email": "info@unicoderns.com",
    "url": "https://unicoderns.com/"
  },
  "main": "index",
  "types": "index.d",
  "scripts": {
    "build": "tsc",
    "prepare": "tsc",
    "test": "nyc --reporter=html --all --require ts-node/register jasmine sources/tests/**/*.test.ts",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/tests/",
      "**/*.d.ts",
      "**/*.js",
      "**/.eslintrc.js",
      "**/.prettierrc.js",
      "**/coverage/**"
    ],
    "reporter": [
      "text-lcov"
    ],
    "all": true
  },
  "bugs": {
    "url": "https://github.com/unicoderns/ORM/issues",
    "email": "info@unicoderns.com"
  },
  "keywords": [
    "orm",
    "sql",
    "database",
    "mapping",
    "postgreSQL",
    "mySQL"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/jasmine": "^3.5.11",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jasmine": "^3.5.0",
    "nyc": "^15.1.0",
    "prettier": "^2.0.5",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6"
  },
  "dependencies": {
    "chalk": "^4.1.1"
  }
